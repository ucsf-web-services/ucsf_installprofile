      $fields = array(
        'body' => $record['body'],
        'info' => $record['info'],
        'format' => $record['format'],
      );
      db_insert('block_custom')
      ->fields($fields)
      ->execute();
      $custom_block = TRUE;
    }

    foreach($record['theme'] as $block_theme){
      $fields = array(
        'module' => $record['module'],
        'delta' => $record['delta'],
        'theme' => $block_theme,
        'status' => $record['status'],
        'weight' => $record['weight'],
        'visibility' => $record['visibility'],
        'region' => $record['region'],
        'pages' => $record['pages'],
        'title' => $record['title'],
        'cache' => $record['cache'],
      );
      db_insert('block')
        ->fields($fields)
        ->execute();
    }
    if($record['css_class'] != '') {
      $fields = array(
        'module' => $record['module'],
        'delta' => $record['delta'],
        'css_class' => $record['css_class'],
      );
      db_insert('block_class')
      ->fields($fields)
      ->execute();
    }
  }
