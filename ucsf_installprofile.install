    variable_set('node_submitted_news', TRUE);
    variable_set('date_format_news', 'ucsf_long_date');

    // Webform
    variable_set('node_submitted_webform', FALSE);
    variable_set('comment_webform', COMMENT_NODE_HIDDEN);


