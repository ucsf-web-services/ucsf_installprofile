  // which is 1.10, and which does not jive well with our theme.
  // [ST 2014/06/19]
  variable_set('jquery_update_jquery_version', '1.5');

  // Theme
  $ucsf_base_settings = variable_get('theme_ucsf_base_settings');
  $ucsf_base_settings['mothership_classes_field_label'] = 0;
